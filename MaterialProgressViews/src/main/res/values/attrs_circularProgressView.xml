    <integer name="CircularProgressView.Defaults.FastSpeed">540</integer>
    <integer name="CircularProgressView.Defaults.SlowSpeed">200</integer>
    <integer name="CircularProgressView.Defaults.SwapAngle">270</integer>
</resources>