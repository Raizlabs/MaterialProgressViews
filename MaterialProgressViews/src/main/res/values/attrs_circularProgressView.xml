        <attr name="circularProgressViewLineThickness" format="dimension"/>
        <attr name="circularProgressViewFastSpeed" format="float"/>
        <attr name="circularProgressViewSlowSpeed" format="float"/>
        <attr name="circularProgressViewSwapAngle" format="float"/>
